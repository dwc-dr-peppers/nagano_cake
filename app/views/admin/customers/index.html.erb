<h3>会員一覧</h3>

<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
  <% @customer.each do |customer| %>
    <tr>
      <td><%= customer.id %></td>
      <td><%= customer.family_name %></td>
      <td></td>
      <td></td>
    </tr>
  <% end %>
  </tbody>
</table>